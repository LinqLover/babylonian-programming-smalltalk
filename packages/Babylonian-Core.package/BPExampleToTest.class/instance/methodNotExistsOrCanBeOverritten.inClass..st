private - serializing
methodNotExistsOrCanBeOverritten: aMethodName inClass: aClass
	"Return true if method does not exist or is generated by this class"

	| method |
	
	method := aClass methodDict at: (aMethodName asSymbol) ifAbsent: nil.
	^ method isNil or: [
		(aClass methodDict at: (aMethodName asSymbol)) getSource asString includesSubstring: (self testComment)]

	